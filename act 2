cristales_preciosos = ["Rubí", "Esmeralda", "Zafiro", "Diamante"]


cristales_unicos = set(cristales_preciosos)


cristales_unicos_lista = lista(cristales_unicos)

print("Lista de cristales preciosos originales:", cristales_preciosos)
print("Lista de cristales preciosos únicos:", cristales_unicos_lista)
